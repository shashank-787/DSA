int print2largest(int arr[], int n) {
	    
	    int max1 = arr[0];
	    for(int i=0 ; i<n ;i++)
	    {
	        if(arr[i] > max1){
	            max1 = arr[i];
	        }        
	    }
	    int max2 = -1;
	    for(int i=0 ; i<n ; i++){
	    
	        if(arr[i] > max2 && arr[i] < max1){
	            max2 = arr[i];
	        }
	    }
	    return max2;
	}
